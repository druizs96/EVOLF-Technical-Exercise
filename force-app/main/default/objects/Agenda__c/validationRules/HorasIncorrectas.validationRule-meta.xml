<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HorasIncorrectas</fullName>
    <active>true</active>
    <errorConditionFormula>Hora_Inicio_Agenda__c &gt;=  Hora_Final_Agenda__c</errorConditionFormula>
    <errorMessage>La hora de inicio de la agenda no puede ser igual ni posterior a la hora de final.</errorMessage>
</ValidationRule>
